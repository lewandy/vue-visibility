{
  "name": "vue-permission-provider",
  "version": "0.0.1",
  "main": "vue-permission-provider.umd.js",
  "module": "dist/vue-permission-provider.esm.js",
  "unpkg": "dist/vue-permission-provider.min.js",
  "browser": {
    "./sfc": "src/permissionProvider.js"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-permission-provider.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-permission-provider.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-permission-provider.min.js"
  },
  "devDependencies": {
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-vue": "^3.0.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  }
}
